{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "shell:allow-open",
    "dialog:default",
    "fs:default",
    "fs:allow-read-file",
    "fs:allow-write-file",
    "fs:allow-read-dir",
    "fs:allow-create",
    "fs:allow-exists",
    "fs:allow-remove",
    "os:allow-platform",
    "opener:default",
    "store:default",
    "core:window:allow-destroy",
    {
      "identifier": "fs:scope",
      "allow": [
        "https://**"
      ]
    },
    "os:default"
  ]
}