{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Free FPS",
  "version": "1.0.0",
  "identifier": "com.undelalune.freefps",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:5173",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Free FPS",
        "width": 450,
        "height": 680,
        "backgroundColor": "#121821",
        "resizable": false,
        "fullscreen": false,
        "center": true,
        "decorations": true,
        "alwaysOnTop": false,
        "skipTaskbar": false,
        "maximized": false,
        "maximizable": false
      }
    ],
    "security": {
      "csp": null
    },
    "macOSPrivateApi": false
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "license": "MIT",
    "copyright": "Â© 2025 undelalune",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}